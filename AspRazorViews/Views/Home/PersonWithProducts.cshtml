@model PersonProducts
<!DOCTYPE html>
<html>
    <head>
        <title>Person Products</title>
        <meta charset="utf-8"/>
        <link rel="stylesheet" href="~/StyleSheet.css"/>
    </head>
    <body>
        <div class="page-content">
            <div class="box">
                <table class="table w-100">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Date Of Birth</th>
                            <th>Gender</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>@Model.Person.Name</td>
                            <td>@Model.Person.DateOfBirth</td>
                            <td>@Model.Person.Gender</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="box">
                <table class="table w-100">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>@Model.Product.ProductId</td>
                            <td>@Model.Product.ProductName</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <a href="/">Home</a>
        </div>
        <script></script>
    </body>
</html>