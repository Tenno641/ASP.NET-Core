@model PersonGrid
<div class="box">
    <h3>@Model.GridTitle</h3>
    <table class="table w-100">
        <thead>
            <tr>
                <th>Name</th>
                <th>Job Title</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Person person in Model.Persons)
            {
                <tr>
                    <td>@person.Name</td>
                    <td>@person.JoBTitle</td>
                </tr>
            }
        </tbody>
    </table>
</div>